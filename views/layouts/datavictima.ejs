	function habilitarLibretaMilitar()
	{
		if (document.getElementById("id_Sexo").value == "Femenino")
		{
			document.getElementById("id_Libretamilitar").disabled=true;
			document.getElementById("id_Libretamilitar").value = "";
			document.getElementById("id_Libretamilitar").style.background = "#C9CACB";
		}else {
			document.getElementById("id_Libretamilitar").disabled=false;
			document.getElementById("id_Libretamilitar").style.background = "";
		}
	}
	
	function habilitarHomicidio()
	{
		if (document.getElementById("id_Homicidio").checked  == true)
		{
			document.getElementById("id_hom_Ano").disabled=false;
			document.getElementById("id_hom_Declarado").disabled=false;
