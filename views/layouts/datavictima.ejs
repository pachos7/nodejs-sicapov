			document.getElementById("id_hom_Declarado").style.background = "";
			document.getElementById("id_hom_Lugardeclarado").style.background = "";
			document.getElementById("id_hom_Estadodeclaracion").style.background = "";
		}
		else
		{
			document.getElementById("id_hom_Ano").disabled=true;
			document.getElementById("id_hom_Declarado").disabled=true;
			document.getElementById("id_hom_Lugardeclarado").disabled=true;
			document.getElementById("id_hom_Estadodeclaracion").disabled=true;	
			
