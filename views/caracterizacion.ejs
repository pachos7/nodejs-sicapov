<%- include layouts/header.ejs %>
    <div class="full-page">
      <h1 class="headline">
        <strong>
          <%=page_title%>
        </strong>
      </h1>
      <div class="">
    	<form method="post" action="/consulta">		
			<div class="mc-field-group" style="margin: 10px 0 0 0;">
			  <input type="text" value="" name="cedula" class="required cedula" id="id_cedula" placeholder="Ingrese cedula"
			  style="width: 237px; border-top-left-radius: 4px; border-top-right-radius: 4px; border-bottom-right-radius: 4px; border-bottom-left-radius: 4px;" required title="Por favor ingresa un numero de cedula!"/>
			  <button type="submit" name="consultar" value="consultar" class="button btn" id="mc-embedded-consultar">
				<span style="margin-top: -1px;">Consultar</span>
			  </button>
			</div>
			<% if(data.length){ %>
			<div class="mc-field-group" style="margin: 10px 0 0 0;">
			  <input type="email" value="" name="EMAIL" class="required email" id="mce-EMAIL" placeholder="Email address"
			  style="width: 237px; border-top-left-radius: 4px; border-top-right-radius: 4px; border-bottom-right-radius: 4px; border-bottom-left-radius: 4px;" />
			</div>
			<div class="mc-field-group">
			  <input type="text" value="" name="FNAME" class="required textInput rounded" id="mce-FNAME" placeholder="First name"
			  style="width: 111px; margin-right: 3px; float: left; border-top-left-radius: 4px; border-top-right-radius: 4px; border-bottom-right-radius: 4px; border-bottom-left-radius: 4px;" />
			</div>
			<div class="mc-field-group">
			  <input type="text" value="" name="LNAME" class="required textInput rounded" id="mce-LNAME" placeholder="Last name"
			  style="width: 111px; border-top-left-radius: 4px; border-top-right-radius: 4px; border-bottom-right-radius: 4px; border-bottom-left-radius: 4px;" />
			</div>
			<div id="mce-responses" class="clear">
			  <div class="response" id="mce-error-response" style="display:none"></div>
			  <div class="response" id="mce-success-response" style="display:none"></div>
			</div>
              
			<div id="resultadobusquedavictima">
				
					<h2>Nombre: <span><%=data[0].nombre %>  <%=data[0].apellido %></span></h2>
            </div>			
			<%} else { %>
					<!-- <h2>No encontre nada</h2> -->
				
			
			<% }  %>
		</form>
      </div>
    </div>
<%- include layouts/footer.ejs %>
